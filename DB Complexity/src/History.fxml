        <buttons>
            <Button mnemonicParsing="false" onAction="#handleButtonClear" text="Очистить" />
          <Button fx:id="fxDelete" disable="true" mnemonicParsing="false" text="Удалить">
               <cursor>
                  <Cursor fx:constant="HAND" />
