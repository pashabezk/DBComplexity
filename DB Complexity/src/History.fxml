            </Button>
            <Button mnemonicParsing="false" onAction="#handleButtonClose" text="Закрыть">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
        </buttons>
      </ButtonBar>
      <TableView fx:id="fxTable" layoutX="36.0" layoutY="58.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="75.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="90.0" />
      <Label alignment="CENTER" layoutX="286.0" layoutY="44.0" text="История" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="30.0">
